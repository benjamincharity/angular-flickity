    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "eslint": "^3.8.1",
    "eslint-config-moment": "^2.1.4",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-angular": "^1.1.1",
